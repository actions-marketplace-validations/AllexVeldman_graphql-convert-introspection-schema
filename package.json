{
  "name": "graphql-convert-introspection-schema",
  "version": "0.0.1",
  "private": true,
  "description": "GitHub Action to convert an introspection schema.json to a .graphql schema",
  "main": "lib/main.js",
  "scripts": {
    "build": "tsc",
    "format": "prettier --write '**/*.ts'",
    "format-check": "prettier --check '**/*.ts'",
    "lint": "eslint src/**/*.ts",
    "package": "ncc build --source-map --license licenses.txt",
    "test": "jest",
    "all": "yarn build && yarn format && yarn lint && yarn package && yarn test"
  },
  "repository": {
    "type": "git",
    "url": "git+https://github.com/AllexVeldman/graphql-convert-introspection-schema.git"
  },
  "keywords": [
    "actions",
    "node",
    "graphql",
    "introspection"
  ],
  "author": "",
  "license": "MIT",
  "dependencies": {
    "@actions/core": "^1.6.0",
    "graphql": "^16.3.0",
    "tunnel": "^0.0.6"
  },
  "devDependencies": {
    "@types/jest": "^27.0.3",
    "@types/node": "^17.0.16",
    "@typescript-eslint/parser": "^5.11.0",
    "@vercel/ncc": "^0.33.1",
    "eslint": "^8.8.0",
    "eslint-plugin-github": "^4.3.2",
    "eslint-plugin-jest": "^26.1.0",
    "eslint-plugin-prettier": "^4.0.0",
    "jest": "^27.5.1",
    "js-yaml": "^4.1.0",
    "prettier": "3.7.4",
    "ts-jest": "^27.1.3",
    "typescript": "^4.5.5"
  }
}
